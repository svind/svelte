{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "es2020",
    "lib": ["es2020", "DOM"],
    "target": "es2020",
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],

      "svind": ["./src/lib"],
      "svind/*": ["./src/lib/*"]
    },
    "rootDirs": ["..", "./types"]
  }
}
